{
  "configurations": [
    {
      "name": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include",
        "/usr/local/include"
      ],
      "defines": [
        "_DEFAULT_SOURCE"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "gcc-x64",
      "compilerArgs": ["-Wall", "-Wextra", "-Werror"]

    }
  ],
  "version": 4
}